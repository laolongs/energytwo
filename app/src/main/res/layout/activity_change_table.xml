<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    tools:context="tties.cn.energy.view.activity.ChangeTableActivity">
    <LinearLayout
        android:orientation="vertical"
        android:layout_above="@+id/electrical_table_confirm"
        android:layout_width="match_parent"
        android:layout_height="match_parent">
        <include layout="@layout/toobar"/>
        <ListView
            android:id="@+id/identity_change_lv"

            android:layout_width="match_parent"
            android:layout_height="match_parent">
        </ListView>
    </LinearLayout>
    <LinearLayout
        android:gravity="center"
        android:id="@+id/electrical_table_confirm"
        android:layout_alignParentBottom="true"
        android:layout_width="match_parent"
        android:background="#607180"
        android:layout_height="@dimen/y50">
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textColor="@color/white"
            android:textSize="17sp"
            android:text="确定使用此电表"/>
    </LinearLayout>
</RelativeLayout>
