<resources>
    <!-- toobar -->
    <style name="toolbar">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">60dp</item>
        <item name="android:background">#F2F2F2</item>
        <item name="android:minHeight">40dp</item>
        <item name="android:theme">@style/toolbar_blue_theme</item>
    </style>
    <!-- toobar -->
    <style name="headtoolbar">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">60dp</item>
        <item name="android:minHeight">40dp</item>
        <item name="android:theme">@style/toolbar_blue_theme</item>
    </style>

    <style name="toolbar_center">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_centerInParent">true</item>
        <item name="android:textColor">@color/black</item>
        <item name="android:layout_gravity">center</item>
        <item name="android:textSize">19sp</item>
    </style>


    <style name="table_dialog_btn_cancel">
        <item name="android:layout_width">40dp</item>
        <item name="android:layout_height">25dp</item>
        <item name="android:layout_centerVertical">true</item>
        <item name="android:layout_marginLeft">20dp</item>
        <item name="android:text">取消</item>
        <item name="android:textSize">12sp</item>
    </style>
    <style name="table_dialog_title_text">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_centerHorizontal">true</item>
        <item name="android:layout_centerVertical">true</item>
        <item name="android:textColor">@color/black</item>
        <item name="android:textSize">18sp</item>
    </style>
    <style name="table_dialog_btn_confirm">
        <item name="android:layout_width">150dp</item>
        <item name="android:layout_height">33dp</item>
        <item name="android:background">@drawable/selector_btn_confirm_click</item>
        <item name="android:textColor">@color/white</item>
    </style>
    <style name="toolbar_right">
        <item name="android:layout_width">16dp</item>
        <item name="android:layout_height">16dp</item>
        <item name="android:layout_centerInParent">true</item>
        <item name="android:layout_gravity">right</item>
        <item name="android:layout_marginRight">15dp</item>
        <item name="android:textSize">19sp</item>
    </style>
    <!-- 个页面 -->
    <style name="login_layout">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">40dp</item>
        <item name="android:paddingTop">2dp</item>
    </style>

    <style name="login_image">
        <item name="android:layout_width">17dp</item>
        <item name="android:layout_height">17dp</item>
        <item name="android:layout_alignParentLeft">true</item>
        <item name="android:layout_marginTop">10dp</item>
        <item name="android:layout_marginLeft">14dp</item>
    </style>

    <style name="login_textview">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_marginLeft">10dp</item>
        <item name="android:layout_marginTop">10dp</item>
        <item name="android:textColor">@color/white</item>
        <item name="android:singleLine">true</item>
    </style>

    <style name="login_image_bottom">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">1dp</item>
        <item name="android:layout_alignParentBottom">true</item>
        <item name="android:background">@color/login_line_bottom</item>
    </style>

    <style name="login_editview">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_marginTop">8dp</item>
        <item name="android:background">@null</item>
        <item name="android:paddingLeft">45dp</item>
        <item name="android:textColor">@color/white</item>
        <item name="android:textColorHint">@color/white</item>
    </style>

    <style name="home_textview">
        <item name="android:layout_width">0dp</item>
        <item name="android:layout_height">match_parent</item>
        <item name="android:textColor">@color/white</item>
        <item name="android:textSize">48px</item>
        <item name="android:gravity">center</item>
        <item name="android:layout_weight">1</item>
    </style>

    <style name="monitor_imageview">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_alignParentRight">true</item>
        <item name="android:layout_marginTop">16dp</item>
    </style>

    <style name="monitor_textview_peak_date">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_alignParentRight">true</item>
        <item name="android:textSize">11sp</item>
    </style>

    <style name="monitor_textview_peak_value">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textColor">@color/black</item>
        <item name="android:textSize">22sp</item>
    </style>

    <style name="monitor_textview_peak_unit">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_marginLeft">5dp</item>
        <item name="android:textColor">@color/gray</item>
        <item name="android:textSize">10sp</item>
    </style>

    <style name="monitor_textview_peak_mark">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textColor">@color/gray</item>
        <item name="android:textSize">11sp</item>
    </style>

    <style name="identity_layout">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">62dp</item>
        <item name="android:paddingLeft">24dp</item>
        <item name="android:gravity">center_vertical</item>
        <item name="android:orientation">horizontal</item>
        <item name="android:background">@drawable/selector_layout</item>
    </style>
    <style name="identity_layout_item">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">0dp</item>
        <item name="android:paddingLeft">24dp</item>
        <item name="android:gravity">center_vertical</item>
        <item name="android:orientation">horizontal</item>
        <item name="android:background">@drawable/selector_layout</item>
    </style>
    <style name="identity_textview">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_marginLeft">@dimen/y24</item>
        <item name="android:textColor">@color/black</item>
        <item name="android:textSize">17sp</item>
    </style>
    <style name="identity_textright">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_marginRight">10dp</item>
        <item name="android:textColor">@color/black</item>
        <item name="android:textSize">17sp</item>

    </style>

    <style name="identity_imageview">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
    </style>
    <!-- 通用 -->
    <style name="layout_radiogroup">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:paddingRight">15dp</item>
    </style>

    <style name="radiogroup_ymwd">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_marginLeft">50dp</item>
        <item name="android:layout_marginRight">50dp</item>
        <item name="android:orientation">horizontal</item>
    </style>

    <style name="radiobutton_ymwd">
        <item name="android:layout_width">50dp</item>
        <item name="android:layout_height">23dp</item>
        <item name="android:layout_marginLeft">7dp</item>
        <item name="android:layout_marginRight">7dp</item>
        <item name="android:background">@drawable/selector_radiobtn_date_dwmy_bg</item>
        <item name="android:button">@null</item>
        <item name="android:gravity">center</item>
        <item name="android:textSize">14sp</item>
        <item name="android:textColor">@drawable/selector_radiobtn_date_dwmy_text</item>
    </style>

    <style name="btn_ymwd">
        <item name="android:layout_width">50dp</item>
        <item name="android:layout_height">23dp</item>
        <item name="android:layout_marginLeft">7dp</item>
        <item name="android:layout_marginRight">7dp</item>
    </style>

    <style name="table_layout">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:paddingTop">5dp</item>
        <item name="android:paddingLeft">10dp</item>
        <item name="android:paddingRight">10dp</item>
        <item name="android:paddingBottom">10dp</item>
    </style>

    <style name="table_layout_title_line">
        <item name="android:layout_width">2dp</item>
        <item name="android:layout_height">15dp</item>
        <item name="android:layout_gravity">center</item>
        <item name="android:layout_marginTop">5dp</item>
        <item name="android:background">@color/statment_layout_title_h_line</item>
    </style>

    <style name="table_layout_title_textview">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_marginLeft">5dp</item>
        <item name="android:layout_marginBottom">5dp</item>
        <item name="android:textColor">@color/black</item>
        <item name="android:textSize">16sp</item>
    </style>

    <style name="table_title_textview">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_marginBottom">5dp</item>
        <item name="android:layout_marginLeft">5dp</item>
        <item name="android:layout_marginTop">5dp</item>
        <item name="android:textColor">@color/black</item>
        <item name="android:textSize">15sp</item>
    </style>

    <style name="table_listview">
        <item name="android:layout_width">fill_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:cacheColorHint">@android:color/transparent</item>
        <item name="android:divider">#00000000</item>
        <item name="android:dividerHeight">0dip</item>
        <item name="android:fadingEdge">none</item>
        <item name="android:listSelector">@android:color/transparent</item>
        <item name="android:scrollbars">none</item>
    </style>

    <style name="table_sum_left">
        <item name="android:layout_width">0dip</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_weight">1</item>
        <item name="android:gravity">left</item>
        <item name="android:paddingBottom">10dip</item>
        <item name="android:paddingTop">10dip</item>
        <item name="android:singleLine">true</item>
        <item name="android:textColor">#000000</item>
        <item name="android:textSize">14sp</item>
        <item name="android:paddingLeft">20dip</item>
    </style>

    <style name="table_sum_right">
        <item name="android:layout_width">0dip</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_weight">1</item>
        <item name="android:gravity">right</item>
        <item name="android:paddingBottom">10dip</item>
        <item name="android:paddingTop">10dip</item>
        <item name="android:singleLine">true</item>
        <item name="android:textColor">#000000</item>
        <item name="android:textSize">14sp</item>
        <item name="android:paddingRight">20dip</item>
    </style>

    <style name="table_mark_left">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_marginLeft">15dp</item>
        <item name="android:layout_marginTop">2dp</item>
        <item name="android:textColor">@color/black</item>
        <item name="android:textSize">12sp</item>
    </style>

    <style name="table_mark_right">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_alignParentRight">true</item>
        <item name="android:layout_marginTop">2dp</item>
        <item name="android:textColor">@color/black</item>
        <item name="android:textSize">12sp</item>
    </style>


    <style name="table_sum">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_marginLeft">50dp</item>
        <item name="android:layout_marginTop">2dp</item>
        <item name="android:textColor">@color/black</item>
        <item name="android:textSize">12sp</item>
    </style>

    <style name="table_mark">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_marginLeft">15dp</item>
        <item name="android:layout_marginTop">2dp</item>
        <item name="android:textColor">@color/black</item>
        <item name="android:textSize">15sp</item>
    </style>

    <style name="cut_off_line_1dp">
        <item name="android:layout_width">fill_parent</item>
        <item name="android:layout_height">@dimen/x1</item>
        <item name="android:background">@color/border</item>
    </style>

    <style name="cut_off_line_5dp">
        <item name="android:layout_width">fill_parent</item>
        <item name="android:layout_height">5dp</item>
        <item name="android:background">@color/border</item>
    </style>

    <style name="cut_off_line_9dp">
        <item name="android:layout_width">fill_parent</item>
        <item name="android:layout_height">9dp</item>
        <item name="android:background">@color/border</item>
    </style>
    <style name="ops_item_top_textview">
        <item name="android:layout_width">0dp</item>
        <item name="android:layout_height">match_parent</item>
        <item name="android:layout_marginLeft">@dimen/x10</item>
        <item name="android:layout_marginTop">@dimen/x10</item>
        <item name="android:textColor">@color/gray</item>
        <item name="android:layout_weight">2</item>
        <item name="android:textSize">15sp</item>
    </style>
    <style name="data_textview">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_marginRight">24dp</item>
        <item name="android:textColor">@color/black</item>
        <item name="android:textSize">25sp</item>
    </style>
    <style name="questions_text_style">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textColor">@color/white</item>
        <item name="android:background">@drawable/questions_text_shop</item>
        <item name="android:gravity">center</item>
    </style>
    <style name="tab_text_siez">
        <item name="android:textSize">20sp</item>

    </style>
    <!--评分进度条-->
    <style name="roomRatingBar" parent="@android:style/Widget.RatingBar">
        <item name="android:progressDrawable">@drawable/atingbar_full</item>
        <item name="android:minHeight">24dip</item>
        <item name="android:maxHeight">24dip</item>
    </style>
    <!-- CustomCheckBox theme -->
    <style name="CustomCheckBoxTheme" parent="@android:style/Widget.CompoundButton.CheckBox">
        <item name="android:button">@drawable/checkbox_style</item>
     </style>
    <style name="dialog" parent="@android:style/Theme.Dialog">
        <item name="android:windowFrame">@null</item>
        <!-- 边框 -->
        <item name="android:windowIsFloating">true</item>
        <!-- 是否浮现在activity之上 -->
        <item name="android:windowIsTranslucent">true</item>
        <!-- 半透明 -->
        <item name="android:windowNoTitle">true</item>
        <!-- 无标题 -->
        <item name="android:windowBackground">@android:color/transparent</item>
        <!-- 背景透明 -->
        <item name="android:backgroundDimEnabled">true</item>

        <item name="android:windowAnimationStyle">@android:style/Animation.Dialog</item>
        <!-- 模糊 -->
    </style>
</resources>
